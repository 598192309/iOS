  project 'Bigjpg/Bigjpg.xcodeproj'
  
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

	pod 'SDWebImage'
  pod 'AFNetworking'
  pod 'Masonry'
  pod 'Reachability'
  pod 'ZLPhotoBrowser'
  pod 'AliyunOSSiOS'
  pod 'JSONModel'
  pod 'SAMKeychain'
  pod 'MJExtension'
  pod 'SVProgressHUD'
  pod 'QMUIKit'
end
